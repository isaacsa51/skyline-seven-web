h1,h2,h4{font-family: var(--Fjalla);text-transform: uppercase; font-weight: bold; color: #242424;}
h1{font-size: 190%;margin-top: 0px;    margin-bottom: 0;}
h2{font-size: 150%; color: #333333; font-weight: normal;}
h4{font-size: 20px; font-weight: normal;}
.carrusel-ofertas{margin: 31px 0 32px;color: #FFFFFF;width: 100%;padding: 0 15px;height: 50px;overflow: hidden;line-height: 50px;float: none;color: #242424;border-top: 1px solid #000;border-bottom: 1px solid #000;text-align: center;clear: both;}
.carrusel-ofertas .swiper-slide{opacity: 0 !important;z-index: -1}
.carrusel-ofertas .swiper-slide-active{opacity: 1 !important;z-index: 1}
#title{font-size: 3.21em;font-weight: normal;line-height: 1.25em;}
.precio{color: #606060;font-size: 1.42em;display: inline-block;}
.special_price{margin:0 5px;text-decoration: line-through;font-size: 1.28em;font-weight: 300;color: #797979;display: inline-block;}
.talla{margin-top: 28px;position: relative;width: 13.6%;float: left;min-width: 100px;}
.talla>p{font-family: 'Roboto',sans-serif; font-size: 0.85em; color: #333333;}
.talla>p>a{font-size: 80%; color: #878787; text-decoration: underline;}
.talla select{
  width: 100%;
    background: #FFFFFF;
    font-size: 1em;
    font-family: arial;
    padding: 0 10px;
    border: 1px solid #9E9E9E;
    border-radius: 0;
    -webkit-appearance: none;
    -moz-appearance: none;
    height: 40px;
    color: #333;
}
.talla button{
  padding: 0 19px;
  margin-left: 3.2%;
  width: 64.5%;
}
.discount{display: none;}
.content-text #add{width: 27.2%;float: left;padding: 0;margin-top: 54px;margin-left: 1.34%;min-width: 200px;cursor: pointer;display: none;}
.disponibilidad{margin-top: 54px;margin-left: 1.34%;min-width: 255px}
.horizontal{clear: both;}
.talla .input.select{width: 100%;float: left;position: relative;}
.talla .input.select:before{pointer-events: none;content: "\e907";position: absolute;bottom: 8px;right: 6px;font-size: 19px;color: #707070;}
.talla .icon-chevron-down{position: absolute;}
.oferta{font-size: 100%;list-style: none;font-size: 17px}
.oferta li{margin-bottom: 7px;list-style: none;}
.oferta p{margin-bottom: 0}
.oferta strong{font-family: var(--roboto);}
.oferta img{margin-right: 14px;}
.oferta .icon-check{font-size: 12px;margin-right: 7px;position: relative;top: 2px}
.oferta .sub-list{margin-left: 21px}
.oferta .sub-list li{margin: 12px 0 11px;}
.subtitle{font-size: 1.42em;font-size: 1.78em;}

.descripcion{margin-bottom: 41px;width: calc(96.8% - 216px);float: left;}
.descripcion p{font-family: var(--roboto);line-height: 1.5em;font-size: 1.07em; color: #000000; margin-top:9px; font-weight: lighter;}
.detalles{width: 216px;float: left;margin-left: 3.2%;margin-top: 36px;}
.detalles p{font-family: var(--roboto); font-size: 98%;font-size: 14px;font-weight: bold; color: #000000;margin-bottom: 0}
.detalles li{list-style: none;margin-bottom: 2.8px;}
.productos{margin-top: 37px;margin-bottom: 250px;clear: both;}
.productos>.row{text-align: center; position: relative;margin: 0;}
.productos>.row>div{padding-left: 0;padding-right: 10px;}
.productos>.row>div:hover img{box-shadow:0px 3px 12px #9a9a9a;}
.productos img{width: 80%;width: 84.7%;}
.productos h3{font-family: var(--Fjalla);font-size: 0.92em;color: #242424; text-transform: uppercase;margin-top: 10px;}
.productos span{font-family: var(--roboto);font-size: 0.92em; color: #454545;}
.productos .subtitle{margin-bottom: 21px}
#suguerencias .open-new-window:last-child{display: none;}
.productos #suguerencias .special_price{color:#797979}
video{position: absolute;left: 0;top: 0;width: 100%;height: 100%;z-index: 1000;}
.full-picture .icon-play{font-size: 40px;color: #000;padding: 10px;position: absolute;bottom: 20px;right: 20px;cursor: pointer;z-index: 999;}
.full-picture .icon-play:hover{color: #fff}
