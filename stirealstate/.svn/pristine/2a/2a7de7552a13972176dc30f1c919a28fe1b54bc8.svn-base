@import url('https://fonts.googleapis.com/css?family=Fjalla+One');
@import url('https://fonts.googleapis.com/css?family=Roboto:300,400,500,700');

/* --CSS Reseter
Basado en: Yahoo UI Reset CSS, Eric Meyer's Reworked Reset y experiencias personales
-- */
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin: 0;padding: 0;border: 0;outline: 0;font-weight: inherit;font-style: inherit;font-size: 100%;font-family: inherit;vertical-align: baseline}
:focus{outline:0}
::-moz-focus-inner{border:0}
table{border-collapse:separate;border-spacing:0}
caption,th,td{text-align:left;font-weight:normal}
a img,iframe{border:none}
/* Fixes incorrect placement of numbers in ol's in IE6/7 */
ul,ol{margin-left:2em}
ol{list-style:decimal}
ul{list-style:disc}
input,textarea,select,button{font-size:100%;font-family:inherit}
select{margin:0}
h1,h2,h3,h4{font-weight:bold;margin-bottom:0.5em}
h1{font-size:1.3em}
h2{font-size:1.2em}
h3{font-size:1.1em}
h4{font-size:1em}
h5{font-size:1em;}
strong{font-weight:bold}
em{font-style:italic}
fieldset{border:1px solid #000}
/* clearfix */
.clearfix:after{content:'.';display:block;height:0;clear:both;visibility:hidden}
.clearfix{display:inline-block}
* html .clearfix{height:1%}
.clearfix{display:block}



/*Icomoon*/
@font-face {
  font-family: 'icomoon';
  src:  url('../fonts/icomoon-lp/icomoon.eot?s1i2ks');
  src:  url('../fonts/icomoon-lp/icomoon.eot?s1i2ks#iefix') format('embedded-opentype'),
    url('../fonts/icomoon-lp/icomoon.ttf?s1i2ks') format('truetype'),
    url('../fonts/icomoon-lp/icomoon.woff?s1i2ks') format('woff'),
    url('../fonts/icomoon-lp/icomoon.svg?s1i2ks#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-document-download:before {
  content: "\e901";
}
.icon-play:before {
  content: "\e900";
}
.icon-player:before {
  content: "\e900";
}



/*style.css*/
body{color: #707070;font-family: 'Roboto', sans-serif;background: #F6F6F6;max-width: 850px;margin: 0 auto;font-weight: 400;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}
figure{margin: 0}
ul li{list-style: none}
ul,ol{margin: 0}
h2,h1,h3,h4,h5,h6{font-size: inherit;margin-bottom: 0;font-weight: normal;}
img{vertical-align: middle}
select::-ms-expand { display: none;}
select { -webkit-appearance:none; -moz-appearance:none; -o-appearance:none; appearance:none; }
/*Generales*/
#container{padding: 33px 0 0}
.btn1{font-family: 'Fjalla One', sans-serif;background: #FF4800;text-align: center;border: none;font-size: 16px;line-height: 40px;color: #FFFFFF;padding: 0 19px;height: 40px;display: inline-block;line-height: 40px;text-transform: uppercase;text-decoration: none;}
.btn1:hover{color: #fff;text-decoration: none;background: #ce3a00}
.title{padding: 0 59px;font-size: 40px;font-family: 'Fjalla One', sans-serif;text-align: center;margin:49px 0 9px;color: #2F2F2F}
.title2{font-size: 20px;color: #2F2F2F;margin-bottom: 13px;text-align: left;font-family: 'Fjalla One', sans-serif;}
.title:first-child{margin-top: 0}
/*Logo*/
.logo{width: 33px;position: absolute;z-index: 2;top:25px;left: 25px;}
/*Video*/
.video-container{position: relative;display: flex;align-items: center; height:auto; overflow:hidden;margin:24px 0 81px;}
.video-container:before{content: '';width: 100%;height: 100%;background: rgba(15,15,15,.44);position: absolute;left: 0;top: 0}
.video-container.hideshadow:before{display:none;}
.video-container .icon-fullscreen{position: absolute;color: #fff;z-index: 2;bottom: 16px;right: 16px;font-size: 42px;}
.video{width: 100%}
.video-text{color: #fff;font-size: 25px;position: absolute;right: 0;left: 0;text-align: center;}
.video-text .title{color: inherit;margin-bottom: 14px;}
.video-text .paragraph{padding: 0 48px}
.video-container img{width: 100%}
.video-container .icon-play{color: #fff;z-index: 1;cursor: pointer;font-size: 58px;margin-top: 19px;display: inline-block;}
/*Text container*/
.text-container{padding: 20px 37px 33px;}
.paragraph{font-size: 20px;line-height: 32px;text-align: center;}
/*Imagen de producto*/
.img-product{width: 100%;margin:-16px 0 36px;}
/*Slide*/
.slide-product{padding: 51px 0 65px;}
.slide-product img{width: 100%;}
.swiper-pagination{position: relative;margin-top: 2px;z-index: 1;bottom: 194px!important;position: absolute;top: inherit!important;}
.swiper-pagination-bullet{width: 10px;height:10px;background: #CFCFCF;opacity: 1;margin: 0 5px!important;}
.swiper-pagination-bullet-active{background: #888888;}
.paragraph-slide{font-size: 20px;color: #2F2F2F;font-weight: 700;text-align: center;height: 64px;margin-top: 41px;overflow: hidden;line-height: 32px;}
.counter{background: rgba(0,0,0,.6);padding: 0 7px;border-radius: 20px;position: absolute;top: 25px;right: 25px;height: 20px;line-height: 20px;font-size: 10px;font-weight: 300;color: #fff;display: inline-block;z-index: 1}
/*Galeria*/
.container-inner{background: #fff;padding: 50px 0 0;text-align: center;}
.gallery{padding: 0 4px;background: #fff;overflow: hidden;/*margin: 16px 0 48px;*/}
.gallery img{width: calc(50% - 2px);float: left;margin-bottom: 4px}
.gallery img:nth-child(2n){margin-left: 4px}
.img-product.last{margin: 30px 0 25px;}
/*Details*/
.details{padding: 15px 29px 16px;text-align: center;}
.details .btn1{margin-top: 31px}
.list {text-align: left;}
.list li{font-size: 18px;list-style: none;margin-bottom: 16px;line-height: 32px;position: relative;padding-left: 12px}
.list li:before{content: '';width: 4px;height: 4px;background: #333333;border-radius: 50%;display: inline-block;left: 0;top: 14px;position: absolute;}
.tofront{z-index:1;}
.logo.tofront{z-index:2;}
.linkadjunto{color:#2E96EB; font-size:16px; text-decoration: none;}
.linkadjunto .icon-document-download{font-size: 22px;position: relative;top: 4px;}
.linkadjunto img{margin-left:6px; height:20px;}
.linkadjunto:after{content:''; background-image:url('../img/pdf-adjunto.png');}
.linkadjunto:visited{color:#2E96EB}
.con-linkadjunto{width:100%; padding:30px 0px; text-align: center;padding: 24px 0px 0;}
.container-inner.last{padding: 31px 0 46px;}

@media screen and (max-width: 382px){
  .paragraph-slide{height: 96px}
  .swiper-pagination{bottom: 222px!important;}
}